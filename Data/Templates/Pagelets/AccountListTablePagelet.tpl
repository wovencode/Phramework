<div class="data-table">

	<table>
		<tbody>
		{slots}
		</tbody>
	</table>
	
	<div class="data-table-footer">
    	<div class="data-table-pagination">
      	<span class="data-table-pagination-label">{LangPage} {now} {LangOf} {total}</span>
      	<a href="{link}&c={pageNumberLeft}" class="link external {disabledLeft}">
      	  <i class="icon icon-prev color-gray"></i>
     	 </a>
     	 <a href="{link}&c={pageNumberRight}" class="link external {disabledRight}">
      	  <i class="icon icon-next color-gray"></i>
      	</a>
		</div>
	</div>
  
</div>